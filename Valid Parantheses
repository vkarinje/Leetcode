class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        ans=[]
        if len(s)%2 !=0:
            return False
        i=0
        while i <len(s):
            if s[i]=="(" or s[i]=="{" or s[i]=="[":
                ans.append(s[i])
                i=i+1
            else:
                if not ans or (s[i]==")" and ans[-1]!="("):
                    return False
                if not ans or (s[i]=="}" and ans[-1]!="{"):
                    return False
                if not ans or (s[i]=="]" and ans[-1]!="["):
                    return False
                ans.pop()
                i=i+1
        return not ans 
                
